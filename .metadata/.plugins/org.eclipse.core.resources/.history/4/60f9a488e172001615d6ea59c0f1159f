<body>
<script>
function identifyType(data){
	document.write("Type of the variable is : "+ typeof data+ "<br>"  );
}

var type1 = 1;
var type2 = "yashas";
var type3 = true;
var type4 = {"name":"yashas"};


identifyType(type1);
identifyType(type2);
identifyType(type3);
identifyType(type4);

</script>
