<title>Variable types</title>
</head>
<body>
<script>
function identifyType(data){
	document.write("Type of the variable is : "+ typeof data+ "<br>"  );
}

var numberType = 1;
var stringType = "yashas";
var booleanType = true;
var objectType = {"name":"yashas"};

identifyType(numberType);
identifyType(stringType);
identifyType(booleanType);
identifyType(objectType);

</script>
</body>
