<body>
<script>

var gameCharacter = {
		name : "Naruto",
		sex : "Male",
		age: 17,
		strengths : ["Ninjutsu","Taijutsu"],		
		weakness : ["Genjutsu"],
		weapon : "Kunai",
		useWeapon : function(){
			document.write(this.name + " used " + this.weapon +"<br>");
		},
		showStrengths : function(){
			document.write(this.name + "'s strengths are " + this.strengths +"<br>");
		}
		
};
var test = "yashas";
document.write("character name "+test+"<br>");
document.write("character name "+gameCharacter.name+"<br>");
document.write("weapon name "+gameCharacter.weapon+"<br>");
gameCharacter.useWeapon();

</script>
</body>
