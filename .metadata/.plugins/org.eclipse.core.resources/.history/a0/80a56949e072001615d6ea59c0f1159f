<body>
<script>
function identifyType(data){
	document.write("Type of the given variable with value "+ data +" is : "+ typeof data+ "<br>"  );
}

identifyType("Yashas");
identifyType(1);
identifyType({"name":"yashas"});
identifyType(true);
</script>
</body>
